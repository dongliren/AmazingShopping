*{
	margin: 0px;
	padding: 0px;
}
.div1{
	/*background: -moz-linear-gradient(left, #FF0000, #F9F900);*/
	background-color:#FCFCFC;
	/*background: -moz-linear-gradient(top, blue, white);*/
	margin: auto;
	width: 160px;
	height: 340px;
	border:1px solid #E6E6E6;
}
.div3{
	margin-top: 7px;
	height: 35px;
	text-align: center;
	font-size: 20px;
}
.div2{
	display: block;
}
.div2_1{
	display: none;
}
.div4{
	display: none;
}
.div4_1{
	display: block;
}
.li{
	list-style-type: none;
	margin-left: 5px;
	font-size: 14px;
    line-height: 29px;
    max-width: 160px;
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
}
.li1{
	text-align: center;
	list-style-type: none;
	font-size: 14px;
    line-height: 29px;
    max-width: 160px;
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
}
.div1 a:link,a:visited{
	text-decoration: none;
	color: #696969;
}
.div1 a:hover{
	text-decoration: none;
	color: crimson;
}
.span_1 {

	cursor: pointer;
    color: #333;
    margin-top: 10px;
    padding-right: 10px;
    padding-left: 10px;
}
