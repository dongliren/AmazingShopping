*{
	margin: 0px;
	padding: 0px;
}
/*头部*/

#head {
	border-bottom: 1px solid #ddd;
	height: 31px;
	width: 1349px;
	background-color: #e3e4e5;
}

#head .w {
	height: 30px;
	line-height: 30px;
	color: #999;
}

.w {
	margin: auto;
	width: 1190px;
}
.Jspan_bor{
	border-bottom: 1px solid red;
}
.fl {
	height: 30px;
	width: 703.11px;
	float: right;
}

#head li {
	font-size: smaller;
	padding-left: 18px;
	display: list-item;
	list-style-type: none;
	float: left;
}

 a {
 	overflow: hidden;
 
	text-decoration: none;
	color: #999;
	cursor: pointer;
}

#head a:hover {
	color: #c81623;
}


/*导航*/

#nav {
	position: relative;
	height: 140px;
	width: 1349px;
}

#nav .w {
	position: relative;
	height: 1190px;
	height: 140px;
}

.logo {
	margin-left: 130px;
	display: block;
	position: absolute;
	z-index: 2;
	left: 0;
	top: -31px;
	width: 190px;
	height: 170px;
	-moz-box-shadow: 0 -12px 10px rgba(0, 0, 0, .2);
	box-shadow: 0 -12px 10px rgba(0, 0, 0, .2);
	background-color: #fff;
}

.search {
	position: relative;
	z-index: 1;
	height: 60px;
}

.search .form {
	position: absolute;
	left: 420px;
	top: 25px;
	width: 550px;
	height: 35px;
}

.text {
	left: 0;
	padding: 4px;
	width: 490px;
	height: 25px;
	border: 1px solid #f10215;
	line-height: 25px;
	font-size: 14px;
	color: #989898;
}

.search .button {
	position: absolute;
	right: 0;
	width: 50px;
	height: 35px;
	line-height: 35px;
	border: none;
	background-color: #f10215;
	font-size: 20px;
	font-weight: 700;
	color: #fff;
}

button {
	cursor: pointer;
}

.searchicon {
	margin-top: 3px;
}

#settleup {
	position: absolute;
	right: 99px;
	top: 25px;
}

#settleup .cw-icon {
	width: 188px;
	height: 33px;
	background-color: #fff;
	text-align: center;
	line-height: 33px;
}

.cw-icon {
	overflow: hidden;
	position: relative;
	z-index: 1;
	float: left;
	border: 1px solid #e3e4e5;
	top: -25px;
	left: 350px;
}

.cw-icon img {
	position: relative;
	float: left;
	left: 40px;
	top: 5px;
}

#settleup .cw-icon a {
	text-align: center;
	color: #f10214;
	text-decoration: none;
}

#w .nav1 {
	overflow: hidden;
	position: absolute;
	clear: left;
	top: 100px;
	left: 200px;
	bottom: 0;
	width: 900px;
	height: 40px;
}

#w .nav1 ul {
	list-style-type: none;
}

#w .nav1 ul li {
	font-family: "微软雅黑";
	display: block;
	width: 65px;
	float: left;
	margin-left: 40px;
	height: 40px;
	line-height: 40px;
	font-size: 16px;
	font-weight: 700;
}







